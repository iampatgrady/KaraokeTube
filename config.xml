<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns   = "http://www.w3.org/ns/widgets"
        xmlns:gap   = "http://phonegap.com/ns/1.0"
        id          = "io.marymary.karaoketube"
        versionCode = "1" 
        version     = "0.0.1" >

    <!-- versionCode is optional and Android only -->

        <name>KaraokeTube</name>

        <description>
            This is a karaoke queuing app built using PhoneGap.
        </description>

        <author href="https://github.com/iampatgrady" email="pat.grady@gmail.com">
            @iampatgrady
        </author>
        <gap:plugin name="net.tunts.webintent" />
        <gap:plugin name="uk.co.ibcomputing.phonegapbuild.plugins.receivesendintents">
            <param name="LAUNCHMODE" value="singleTop" />
        </gap:plugin>
        
        <feature name="debug-server" required="true">
            <param name="domain" value="http://192.168.1.9:8080"/>
            <param name="key" value="io.marymary.karaoketube"/>
        </feature>
    </widget>

